<div id="<%= dom_id category %>">
  <div class="index_box">
    <p>
      <strong>ジャンル：</strong>
      <span class="next_colon"><%= category.genre.name %></span>
    </p>
    <p>
      <strong>カテゴリ名：</strong>
      <span class="next_colon"><%= category.name %></span>
    </p>
    <div class="links">
      <%= link_to "編集", edit_category_path(category) %>
      <%= button_to "削除", category, method: :delete %>
    </div>
  </div>
</div>
